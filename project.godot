; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="蓝板系统"
run/main_scene="res://entry/main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://assets/texture/icon.png"

[autoload]

PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
SoundManager="*res://addons/sound_manager/sound_manager.gd"
Paths="*res://preloaded/paths.gd"
Tables="*res://preloaded/tables.gd"

[debug]

gdscript/warnings/untyped_declaration=1

[display]

window/size/viewport_height=640
window/size/resizable=false
mouse_cursor/custom_image="res://assets/texture/cursor.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/YATI/plugin.cfg", "res://addons/nklbdev.importality/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/sound_manager/plugin.cfg")

[global_group]

main=""

[gui]

theme/custom_font="res://assets/font/fusion-pixel-12px-monospaced-zh_hans.ttf"

[rendering]

textures/canvas_textures/default_texture_filter=0
